{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appName": {
      "value": "sharedo-docx-converter"
    },
    "environment": {
      "value": "prod"
    },
    "location": {
      "value": "australiaeast"
    },
    "acrName": {
      "value": "docspectiveacr"
    },
    "acrResourceGroup": {
      "value": "Docspective"
    },
    "createAcr": {
      "value": false
    },
    "cpu": {
      "value": "1.0"
    },
    "memory": {
      "value": "2.0Gi"
    },
    "minReplicas": {
      "value": 2
    },
    "maxReplicas": {
      "value": 10
    },
    "logRetentionDays": {
      "value": 90
    },
    "enableAppInsights": {
      "value": true
    },
    "appEnv": {
      "value": "production"
    },
    "debugMode": {
      "value": "false"
    },
    "logLevel": {
      "value": "INFO"
    },
    "maxFileSize": {
      "value": "20971520"
    },
    "conversionTimeout": {
      "value": "120"
    },
    "confidenceThreshold": {
      "value": "95"
    },
    "apiKeyEnabled": {
      "value": "true"
    },
    "apiKey": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/{subscription-id}/resourceGroups/Docspective/providers/Microsoft.KeyVault/vaults/docspective-kv"
        },
        "secretName": "docx-converter-api-key"
      }
    },
    "enableRedis": {
      "value": true
    },
    "redisHost": {
      "value": "docspective-redis.redis.cache.windows.net"
    },
    "redisPassword": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/{subscription-id}/resourceGroups/Docspective/providers/Microsoft.KeyVault/vaults/docspective-kv"
        },
        "secretName": "redis-password"
      }
    },
    "metricsEnabled": {
      "value": "true"
    },
    "rateLimitEnabled": {
      "value": "true"
    },
    "rateLimitRequests": {
      "value": "1000"
    },
    "rateLimitPeriod": {
      "value": "60"
    }
  }
}